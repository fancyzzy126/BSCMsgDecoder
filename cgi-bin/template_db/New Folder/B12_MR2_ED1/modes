NEWMODE M_IPP_IW_MSG_HEAD = STRUCT(
         B_LINK               INT,
         B_MINI_HEAD          M_MINI_HEAD,
         CASE OF
          :B_MSG_DESTINATION    M_LCE_ID,      /* NORMAL MSG BUF HDR */
           B_MSG_DISCRIMINATOR  INT      END
          :B_MB_PROCESS_ID      M_PROCESS_ID   /* NORMAL MSG BUF HDR */
           END
          :B_IPP_DESTINATION    BIT(12)  PACK, /* IPP_IW-DEST LCE_ID */
           B_IPP_PATH_CODE      BIT(4)   PACK,
           B_IPP_DISCRIMINATOR  INT      END
          :B_IPP_OBC_DEST       M_LCE_ID,      /* OBC_DEST IDENTITY  */
           B_IPP_OBC_ID         M_OBC_ID,
           B_IPP_PKT_TYPE       BIN (8)  END
         ESAC,
         CASE OF
          :B_IPP_IW_FLAGS       M_IPP_IW_FLAGS, /* TOWARDS IPP_IW */
           B_IPP_IW_USER        M_IPP_IW_USER,
           B_IPP_IW_ORIG_RGSM   M_IPP_IW_USER END
          :B_IPP_IW_FLAG_RET    M_IPP_IW_FLAGS, /* FROM IPP_IW    */
           B_ORIG_OBC           M_OBC_ID,
           B_ORIG_LCE           M_LCE_ID      END
         ESAC);

