  NEWMODE M_PAGING  = STRUCT (
    B_FMM_ID         M_FMM_ID,
    B_IMSI           M_IMSI_ARRAY ,
    /* IMSI PARA TO CALCULATE THE PAGING GROUP      VAR MAX 9 */
    /*   LENGTH(1) + TYPE OF ID.(4 BITS) + DIGITS(4 BITS  )   */
    B_MS_IDENT       M_MS_IDENT_IE,
    /* MOBILE IDENTITY  I.E.  CONTENT 4.08                    */
    /* THE FORMAT IS   :  INFO ELEMENT ID.         1          */
    /*                    LENGTH                   1          */
    /*                    MS_IDENTITY          MAX 8          */
    B_ALL_CELLS      BOOL,
    /* TRUE :PAGING TO ALL CELLS ,B_CELL_MAP IS NOT RELEVANT  */
    /* FALSE:B_CELL_MAP FIELD IS RELEVANT                     */
    B_GPRS_PAGING M_BYTE,
    /* H'23 :  CS PAGING FOR GPRS, H'24 : PS PAGING FOR GPRS  */
    /* H'FF :  PAGING FOR GSM*/
    B_CELL_MAP       M_CELL_MAP,
    /* TRUE ON POSITION X, MEANS PAGE CELL_IDX X              */
    B_CHANNEL_NEEDED M_CHANNEL_NEEDED,
    B_PRIORITY       M_BYTE,
    B_MSC_SBL      M_SBLN, /* ADD FOR B11 A-FLEX */
    B_SEND BOOL              /* ADD FOR B11 A-FLEX */
);

