NEWMODE M_BSIP_SERV_FLD = STRUCT(
               B_PROT_DISCR       BIT (8),
               B_NPDU_DISCR       BIN (8),
           CASE B_NPDU_DISCR OF
         (M_POS_ACK_NPDU,M_NEG_ACK_NPDU):
                B_ACK_HDR      M_REF_HDR END
         (M_CEREQ_NPDU):
                B_CEREQ_HDR    M_CEREQ_HDR END
         (M_CECNF_NPDU):
                B_CECNF_HDR    M_CECNF_HDR END
         (M_RELAY_NPDU):
                B_RELAY_HDR    M_RELAY_HDR END
         (M_TRREQ_NPDU,M_TRRES_NPDU):
                B_TRANSL_HDR   M_TRANSL_HDR END
         (M_NORM_DATA_NPDU,M_SEGM_DATA_NPDU,M_EXP_DATA_NPDU):
                B_DATA_HDR     M_DATA_HDR END
         (M_RSREQ_NPDU,M_DISC_NPDU):
                B_RES_DISC_HDR M_RES_DISC_HDR END
         (M_RSCNF_NPDU):
                B_RESCNF_HDR   M_REF_HDR END
         (M_TSTREQ_P_NPDU,M_TSTREQ_N_NPDU,M_TSTCNF_P_NPDU,
          M_TSTCNF_N_NPDU):
                B_TST_HDR      M_TST_HDR END
         (M_UDT_NPDU,M_NOTICE_NPDU):
                B_UDT_HDR      M_UDT_HDR END
         (M_SCRT_XLT):
                B_SCRT_XLT_HDR M_SCRT_XLT_HDR END
         (M_SCRT_XLTED):
                B_SCRT_XLD_HDR M_SCRT_XLD_HDR END
         (M_REQ1_NPDU,M_REQ2_NPDU,M_REPLY_NPDU):
                B_ISUP_HDR     M_ISUP_HDR END
         (M_STATE_NPDU,M_PC_STATE_NPDU,M_COORD_NPDU):
                B_SCMG_HDR     M_SCMG_HDR END
         (M_SCMG_RT):
                B_SCMG_RT_HDR  M_SCMG_RT_HDR END
         (M_SCCP_REROUTED):
                B_SCCP_RER_HDR M_SCCP_RER_HDR END
         (ELSE):
                B_DUMMY        BIN(8) END
           ESAC);

