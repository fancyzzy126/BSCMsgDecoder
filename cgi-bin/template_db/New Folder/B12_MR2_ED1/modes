                                  E_F_CANCEL_REQ,
                                  E_F_CANCEL_RSP,
                                  E_F_RECOVER_RSP,
/* ACSE PRIMITIVES (FROM SUPERVIS.)*/
                                  E_A_ASSO_IND, /*   1ST PART OF PRIM */
                                  E_INIRQ,      /*    = A_SEGM_DATA_I */
                                  E_A_ABIN,     /* A_ABORT WITHOUT UD */
                                  E_UABRQ,      /*   A_ABORT_IND +UD  */
                                  E_PABRQ,      /*   A_ABORT_IND +UD  */
                                  E_A_PABIN,    /*   A_P_ABORT_IND    */
/* ACSE RECEIVED PRIMITIVES */
                                  E_AC_RELEASE,/* A_RELEASE WITHOUT UD*/
                                  E_TERRQ,     /* A_RELEASE WITH UD   */
/* PL RECEIVED PRIMITIVES */
                                  E_PL_D_FLOW_CONTROL,
                                  E_CANRQ,       /* = PL_D_RESYNC IND */
                                  E_CANRP,       /* = PL_D_RESYNC CNF */
                                  E_P_TOKIN,     /* = PL_D_TOKEN_GIVE */
                                  E_P_SYMCF, /* = PL_D_SYNC_MINOR CNF */
                                  E_P_SYMIN, /* = PL_D_SYNC_MINOR IND */
/* FPDUS FROM PL_D_DATA_TRANS */
/*        AND PL_D_SEGM_DATA  */
/* ( WITHOUT CANCEL RQ/RP )   */
                                  E_DATIN,
                                  E_GRPRQ,
                                  E_REARQ,
                                  E_WRTRQ,
                                  E_DAERQ,
                                  E_TRERQ,
                                  E_RECRQ,
/* EFS LOCAL SIGNALS */
                                  E_L_GIVEUP,
                                  E_L_ERROR2,
/* EFS FLOW CONTROL */
                                  E_F_GO,
/* END EXTERNAL FILE SERVICE INTERFACE & ACSE, PL SERVICE INTERFACE  */
/* OPERATING SYSTEM  */
                                  E_OS_TIMER_EX
                                 );

