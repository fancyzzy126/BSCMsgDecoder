SYNMODE M_AC_SDU_ID = RANGE( E_AC_ASSOCIATE_REQ: E_AC_FORCE_RELEASE );

NEWMODE M_AC_SERVICE = STRUCT
 ( B_SDU_ID                M_AC_SDU_ID,     /* SERVICES OF ENTITY X   */
   B_CTR_INFO              M_OSI_CTR_INFO,  /*          CONTROL FLAGS */
   B_CON_REF               M_OSI_REFERENCE, /*   CONNECTION REFERENCE */
   B_BITMAP                M_OSI_BITMAP,    /* OPT PARAMETER PRESENCE */
   B_UB_ENTRY              M_OSI_UD_REF,    /* REFER TO THE USER DATA */
   B_OTHER_ID              M_PROCESS_ID,    /* ID OF  SENDING PROCESS */
   CASE B_SDU_ID OF
   ( E_AC_ASSOCIATE_REQ,
     E_AC_ASSOCIATE_IND) : B_AC_ASSOCIATE_INIT  M_AC_ASSOCIATE_INIT  END
   ( E_AC_ASSOCIATE_RSP,
     E_AC_ASSOCIATE_CNF) : B_AC_ASSOCIATE_RESP  M_AC_ASSOCIATE_RESP  END
   ( E_AC_RELEASE_REQ,
     E_AC_RELEASE_IND )  : B_AC_RLS_REQ         M_AC_RLS_REQ         END
   ( E_AC_RELEASE_RSP,
     E_AC_RELEASE_CNF )  : B_AC_RLS_RSP         M_AC_RLS_RSP         END
   ( E_AC_ABORT_REQ,
     E_AC_ABORT_IND )    : B_AC_ABORT           M_AC_ABORT           END
   ( E_AC_P_ABORT_IND,
     E_AC_FORCE_RELEASE) : B_AC_P_ABORT         M_AC_P_ABORT         END
     ESAC
    );

