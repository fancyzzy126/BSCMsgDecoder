      ( E_TL_EDATA )        : B_ED_PARMS           M_EXP_DATA      END
      ( E_TL_REL )          : B_REL_PARMS          M_TL_REL_PARMS  END
      ( ELSE )              :  /* NO ADDITIONAL PARAMETERS */
                              B_DUMMY              RANGE(0:255)    END
        ESAC);

