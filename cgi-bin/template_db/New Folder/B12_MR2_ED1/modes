   B_UB_ENTRY              M_OSI_UD_REF,    /* REFER TO THE USER DATA */
   B_OTHER_ID              M_PROCESS_ID,    /* ID OF  SENDING PROCESS */
   CASE B_SDU_ID OF
   ( E_RO_GO,
     E_RO_NOGO,
     E_RO_OPEN,
     E_RO_CLOSE)       :   B_DUMMY              M_OSI_DUMMY          END
   ( E_RO_INVOKE_REQ,
     E_RO_INVOKE_IND ) :   B_RO_INVOKE          M_RO_INVOKE          END
   ( E_RO_RESULT_REQ,
     E_RO_RESULT_IND ) :   B_RO_RESULT          M_RO_RESULT          END
   ( E_RO_ERROR_REQ,
     E_RO_ERROR_IND )  :   B_RO_ERROR           M_RO_ERROR           END
