               END
               :B_NETADDR M_NETADDR     /* FOR MULTIPOL PURPOSE       */
               END
             ESAC,
             CASE OF
               :B_DISCRIMINATOR INT  /* DISCRIMINATOR IF BASIC-FOR */
               END
               :B_MSG_IDENTITY  INT  /* USED FOR OSNC              */
               END
               :B_FOR_ATTRIBUTE BIN(8),
                B_SUBGROUP      BIN(8)  /* DISCRIMIN. FOR BASIC GROUP*/
               END
               :B_FMM_SELECTOR  INT  /* FMM_SELECTOR FOR BASIC-TO.  */
               END
             ESAC);

NEWMODE M_MB_OWNER = STRUCT (CASE OF
                               : B_FMM_ID M_FMM_ID END
                               : B_SSM_ID M_SSM_ID END
                             ESAC)
/* USED TO DEFINE THE OWNER FIELD IN RESOURCES CLAIMED FROM THE OS. */
/* EXAMPLE OF A MODE FOR A USER BUFFER:                             */
/* NEWMODE M_MY_BUFFER_MODE = STRUCT (                              */
/*                                   B_MB_OWNER M_MB_OWNER,         */
/*                                   USER DEFINITION ...            */
/*                                                      );          */
/*                                                                  */
/* FOR A MESSAGE:                                                   */
/* THE OWNER FIELD IS ACCESSED BY                                   */
/*  Z_MSG_PTR -> M_MSG_HEAD.B_MB_OWNER.                             */
/*                                                                  */
;

