                /*03 H'03*/ E_HWF_GS2_SW,
                /*04 H'04*/ E_HWF_SPARE_4,
                /*05 H'05*/ E_HWF_CLK_A_GEN,
                /*06 H'06*/ E_HWF_CLK_B_GEN,
                /*07 H'07*/ E_HWF_CLK_A1_REP,
                /*08 H'08*/ E_HWF_CLK_A2_REP,
                /*09 H'09*/ E_HWF_CLK_A3_REP,
                /*10 H'0A*/ E_HWF_CLK_A4_REP,
                /*11 H'0B*/ E_HWF_CLK_B1_REP,
                /*12 H'0C*/ E_HWF_CLK_B2_REP,
                /*13 H'0D*/ E_HWF_CLK_B3_REP,
                /*14 H'0E*/ E_HWF_CLK_B4_REP,
                /*15 H'0F*/ E_HWF_SPARE_15,
                /*16 H'10*/ E_HWF_SPARE_16,
                /*17 H'11*/ E_HWF_SPARE_17,
                /*18 H'12*/ E_HWF_SPARE_18,
                /*19 H'13*/ E_HWF_SPARE_19,
                /*20 H'14*/ E_HWF_BCLK_A_GEN,
                /*21 H'15*/ E_HWF_BCLK_B_GEN,
                /*22 H'16*/ E_HWF_BCLK_A_REP,
                /*23 H'17*/ E_HWF_BCLK_B_REP,
                /*24 H'18*/ E_HWF_BCLK_SYS_SC /* AND CLK_A_EXTRACT */,
                /*25 H'19*/ E_HWF_BCLK_RACK_SC,
