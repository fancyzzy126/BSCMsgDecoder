                          B_ADDRESS_INFO BIN(8)      ,
                          CASE B_ADDRESS_INFO OF
                          (67,                    /* B'01000011 */
                            3):                   /* B'00000011 */
                             /* GLOBAL TITLE INDICATOR 0 */
                             /* SSN AND POINTCODE PRESENT*/
                             B_SSN_PC       M_OSI_SSN_PC END
                          (66):                   /* B'01000010 */
                             /* SSN PRESENT (TERMINATING)*/
                             B_SSN          M_OSI_SSN END
                          (82,                    /* B'01010010 */
                           18):                   /* B'00010010 */
                             /* GLOBAL TITLE INDICATOR 4 */
                             B_GT_IND_4   M_OSI_GT_IND_4 END
                          (4):                    /* B'00000100 */
                             /* RED BOOK IMPLEMENTATION */
                             /* GLOBAL TITLE INDICATOR 1 */
                             B_GT_IND_1   M_OSI_GT_IND_1 END
                          (ELSE):/* NO OTHER CASES ALLOWED */
                             B_DUMMY      RANGE (0:255) END
                         ESAC);

