                                  /* 'NON-CONFIRMED'             */
      B_INVOKE_ID INT,          /* INVOKE IDENTIFIER             */
      B_LINKED_ID INT,          /* LINKED IDENTIFIER IF AVAIL.   */
      B_ERROR_VAL M_CM_ERROR_VAL ); /* ERROR VALUE IF AVAIL.     */

   NEWMODE M_CM_ERROR_TYPE = SET (
                           E_INV_REJECT_TYPE,
                           E_GEN_REJECT_TYPE,
                           E_LOC_REJECT_TYPE,
                           E_ERROR_TYPE) ;

