                                  E_I_CANCEL_REQ,
                                  E_I_CANCEL_IND,
                                  E_I_CANCEL_RSP,
                                  E_I_CANCEL_CNF,
                                  E_I_RECOVER_IND,
                                  E_I_RECOVER_RSP,
/* INTERNAL LOCAL SIGNALS */
                                  E_L_ERRABT,
                                  E_L_PABORT,
                                  E_L_P_ABORT_IND,
/* INTERNAL FLOW CONTROL EVENTS */
                                  E_OPEN_PL_WINDOW, /* COMMAND */
                                  E_CLOSE_PL_WINDOW,
                                  E_OPEN_EFS_WINDOW,
                                  E_CLOSE_EFS_WINDOW,
/* END INTERNAL FILE SERVICE INTERFACE */
/* EXTERNAL FILE SERVICE INTERFACE & ACSE, PL SERVICE INTERFACE */
/* EFS RECEIVED PRIMITIVES */
                                  E_F_INITIALIZE_RSP,
                                  E_F_SELOP_RSP,
                                  E_F_DATA_REQ_1,
                                  E_F_DATA_REQ_C,
                                  E_F_DATA_END_REQ,
                                  E_F_TRF_END_REQ,
                                  E_F_TRF_END_RSP,
