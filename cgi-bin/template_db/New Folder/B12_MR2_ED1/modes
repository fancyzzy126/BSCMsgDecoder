NEWMODE M_ANV_IDENTIFICATION = ARRAY (1:33) CHAR
/* USED BY MODULES TO PUT THE ANV ID IN THE GLS.                    */
/* 'ANV_ID = SHID(8C).CYPHVVPP.YYMMDD'                              */
/* -SHID(8C) = SHORT IDENTIFICATION, 8 CHARS LONG.                  */
/* -CYPHVVPP: CYPH = CYPHER OF THE MODULE LINKED OBJECT (GSM NAME)  */
/*          : VV = VARIANT OF THE MODULE LINKED OBJECT (GSM NAME)   */
/*          : PP = PCS OF THE MODULE LINKED OBJECT (GSM NAME)       */
/* -YYMMDD = LAST UPDATED DATE OF THE SOURCE.                       */
;

NEWMODE M_AOIP_CAUSE = SET(
         E_AUIP_DUMMY,         /*0*/
         E_NO_FREE_RES,        /*1*/
         E_TOO_MANY_MGW,       /*2*/
         E_CALLID_ALERADY_USED,/*3*/
         E_STATE_INCONSISTANT  /*4*/
                 );

NEWMODE M_AOIP_LOCATION = STRUCT(
        B_AOIP_LOCATION_TYPE  M_AOIP_LOCATION_TYPE,
        B_DUMMY               M_BYTE,
        CASE OF
         :B_TRAU_ADDRESS     M_TP_LOCATION   END
           /* B_AOIP_LOCATION_TYPE = E_TRAU_ADDR*/
         :B_TRAUP_ADDRESS    M_TRAUP_ADDRESS END
           /* B_AOIP_LOCATION_TYPE= E_TRAUR_ADDR*/
         :B_RTP_ADDRESS      M_RTP_ADDRESS   END
           /* B_AOIP_LOCATION_TYPE = E_RTP_ADDR*/
         :B_TIC_ADDRESS      M_TIC_ID        END
        ESAC);

NEWMODE M_AOIP_LOCATION_TYPE = SET(
   E_TRAU_ADDR,    /* 0 */
   E_TRAUP_ADDR,   /* 1 */
   E_RTP_ADDR,     /* 2 */
   E_TIC_ADDR      /* 3 */
);

