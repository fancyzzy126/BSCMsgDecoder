        B_BEQ_OR_SECTORS        M_MUX_SCHEME_BEQ_OR_SECTORS,
        CASE B_BEQ_OR_SECTORS OF
        (E_MUX_SCHEME_BEQ_BALANCED):
                B_MUX_SCHEME_BEQ        M_MUX_SCHEME    PACK,
                B_SPARE                 M_40BITS        END
        (E_MUX_SCHEME_SECTORS_BALANCED):
                B_MUX_SCHEME_SECTORS   M_MUX_SCHEME_SECTORS_ARRAY END
        ESAC);

SYN M_MUX_RULE_STATISTIC_MUX_16        M_MUX_RULE             = 3 ;

SYN M_MUX_RULE_STATISTIC_MUX_64        M_MUX_RULE             = 2 ;

SYNMODE M_MUX_SCHEME           = SET (
    E_SCH_DUMMY                        /* VALUE = 0 */,
    E_SCH_1_1                          /* VALUE = 1 */,
    E_SCH_2_1                          /* VALUE = 2 */,
    E_SCH_3_1                          /* VALUE = 3 */,
    E_SCH_4_1                          /* VALUE = 4 */
    );

NEWMODE M_MUX_SCHEME_BEQ_OR_SECTORS = SET(
                                        E_MUX_SCHEME_BEQ_BALANCED,
                                        E_MUX_SCHEME_SECTORS_BALANCED);

NEWMODE M_MUX_SCHEME_SECTORS_ARRAY = ARRAY(1:6) M_MUX_SCHEME;

