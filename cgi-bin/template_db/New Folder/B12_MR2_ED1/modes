                                 B_OPER_VAL INT,
                                 B_INVOKE_ID INT ,
                                 B_PRIOR M_RO_PRIOR );

   NEWMODE M_RO_RJCT = STRUCT (
                     B_RJCT_PROBLEM M_RO_RJCT_PROBLEM ,
                     CASE B_RJCT_PROBLEM OF
                     (E_RO_GENERAL_PROBLEM):
                             B_GEN_PROBLEM M_RO_GEN_PROBLEM END
                     (E_RO_U_INVOKE_PROBLEM):
                             B_INV_PROBLEM M_RO_U_INV_PROBLEM END
                     (E_RO_U_RETURN_RSLT_PROBLEM):
                             B_RTN_RSLT_PROBLEM
                                M_RO_U_RTN_RSLT_PROBLEM END
                     (E_RO_U_RETURN_ERROR_PROBLEM):
                             B_RTN_ERR_PROBLEM
                                M_RO_U_RTN_ERR_PROBLEM END
                     (E_SMAE_LOCAL_PROBLEM):
                             B_LOC_PROBLEM M_SMAE_LOC_PROBLEM END ESAC)
                                ;

   NEWMODE M_RO_RJCT_PROBLEM = SET (E_RO_GENERAL_PROBLEM,
