NEWMODE M_RESULT_CODE = SET (
  /* GENERAL RESULT CODE FOR RESPONSE/CONFIRM MESSAGES.               */
  /* THE FIRST FOUR CODES ARE NOT RELATED TO BSC SOFWARE FAULTS, THE  */
  /* OTHER FOUR ARE RELATED TO SOFTWARE FAULTS.                       */
  /* PREFERRED POSITION: FIRST FIELD IN THE MESSAGE BUFFER.           */
  /* SPACE USE: ONE BYTE.                                             */
  E_RC_SUCCESSFUL,    /* ALL OK.                                      */
  E_RC_UNSUCCESSFUL,  /* NOT OK: NOT RELATED TO SOFTWARE FAULT.       */
  E_RC_UNSUCC_SPARE,  /* SPARE UNSUCCESSFUL RESULT.                   */
  E_RC_ABORT_REQUEST, /* THE JOB HAS BEEN CANCELLED BY THE REQUESTOR. */
  E_RC_SWF_INP_DATA,  /* SOFTWARE FAULT: INPUT DATA IS NOT CORRECT.   */
                      /*   SENDER MODULE (PROBABLY) NEEDS CORRECTION. */
  E_RC_SWF_INTERNAL,  /* SOFTWARE FAULT: THE MODULE FINDS ITSELF      */
                      /*   FAULTY, AND NEEDS (PROBABLY) CORRECTION.   */
  E_RC_SWF_ENVIR,     /* SOFTWARE FAULT: ENVIRONMENT ERROR. E.G. NO   */
                      /*   USER BUFFERS, JOB STOPS.                   */
  E_RC_SWF_OTHER      /* SOFTWARE FAULT: OTHERS.                      */
); /* --------------------------------------------------------------- */

