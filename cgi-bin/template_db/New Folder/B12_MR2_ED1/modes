                            E_IE_ADD   );

NEWMODE M_IE_AREA     = STRUCT(
         CASE OF
           :B_IE_DISCR          RANGE (0:255),
            B_IE_LEN            RANGE (0:255) END
/* ADDRESSES FOR BOTH CONNECTION ORIENTED AND -LESS SERVICES */
/*    (M_IE_CALLED_NPAI M_IE_CALLING_NPAI,M_IE_RESPONDING_NPAI):    */
           :B_IE_DISCR_NI       RANGE (0:255),
            B_IE_LEN_NI         RANGE (0:255),
            B_IE_NSAP_INF       M_OSI_NSAP    END
/*    (M_IE_SEGM_INFO):      */
           :B_IE_DISCR_SI       RANGE (0:255),
            B_IE_LEN_SI         RANGE (0:255),
            B_IE_SEGM_INF       M_SEGM_INF    END
/* INFO EXCHANGED DURING ERROR OR RELEASE SITUATIONS */
/* ALSO APLLICABLE FOR IE'S : M_IE_RES_INFO, M_IE_RTRN_REASON */
/*                            AND M_IE_TRANS_FAIL             */
/*    (M_IE_REL_INFO):       */
           :B_IE_DISCR_CI       RANGE (0:255),
            B_IE_LEN_CI         RANGE (0:255),
            B_IE_CAUSE_INFO     M_CAUSE_INF   END
/* PEER INFO TO BE EXCHANGED */
/*    (M_IE_PEER_SPEC_DATA): */
           :B_IE_DISCR_PI       RANGE (0:255),
            B_IE_LEN_PI         RANGE (0:255),
            B_IE_PEER_INFO      M_PEER_INF    END
/* SPECIFIC INFO TO SUPPORT INTERFACE TOWARDS X25 */
/*    (M_IE_DEST_ID):        */
           :B_IE_DISCR_DI       RANGE (0:255),
            B_IE_LEN_DI         RANGE (0:255),
            B_IE_DEST_INFO      M_DEST_INF    END
/* SCCP MANAGEMENT INFO */
/*    (M_IE_N_STATE ,M_IE_N_COORD): */
           :B_IE_DISCR_ST       RANGE (0:255),
            B_IE_LEN_ST         RANGE (0:255),
            B_IE_N_STATE        M_N_STATE     END
/*    (M_IE_N_PC_STATE):     */
           :B_IE_DISCR_PS       RANGE (0:255),
            B_IE_LEN_PS         RANGE (0:255),
            B_IE_N_PC_STATE     M_N_PC_STATE  END
/* NO ROUTING INFO AVAILABLE IN DESTINATION CE */
/*    (M_IE_REROUTE):        */
           :B_IE_DISCR_RP       RANGE (0:255),
            B_IE_LEN_RP         RANGE (0:255),
            B_IE_REROUTE_PTR    PTR           END
/* SPECIFIC ISDN INFO */
/*    (M_IE_OPC):            */
           :B_IE_DISCR_OP       RANGE (0:255),
            B_IE_LEN_OP         RANGE (0:255),
            B_IE_OPC            BIN(14)       END
/* ROUTING INFO AVAILABLE IN DESTINATION CE */
/*    (M_IE_DEST_IDX):       */
           :B_IE_DISCR_DE       RANGE (0:255),
            B_IE_LEN_DE         RANGE (0:255),
            B_IE_DEST_IDX       M_DEST_IDX    END
/*    (M_IE_DEST_PTR):       */
           :B_IE_DISCR_DP       RANGE (0:255),
            B_IE_LEN_DP         RANGE (0:255),
            B_IE_DEST_PTR       M_DEST_PTR    END
/* SPECIFIC INFO NETWORK LAYERS */
/*    (M_IE_NW_TYPE):        */
           :B_IE_DISCR_NT       RANGE (0:255),
            B_IE_NW_TYPE        M_OSI_NW_TYPE END
/*    (M_IE_DISTR_IDX):      */
           :B_IE_DISCR_DS       RANGE (0:255),
            B_IE_DISTR_IDX      M_OSI_DISTR_IDX END
/*    (M_IE_IMPORTANCE):     */
           :B_IE_DISCR_IM       RANGE (0:255),
            B_IE_IMPORTANCE     M_OSI_ASSO_IMP_LEV END
/* SPECIFIC CONNECTION-LESS SERVICE INFO */
/*    (M_IE_SEQUENCE_CTL):   */
           :B_IE_DISCR_SQ       RANGE (0:255),
            B_IE_SEQ_CNTRL      M_SLS END
/*    (M_IE_INT_SSN): /. IDENTITY USER FOR TCAP */
           :B_IE_DISCR_SS       RANGE (0:255),
            B_IE_SSN            M_SSN         END
/* CONTAINS BOOLEANS USED TO SUPPORT THIS INTERFACE */
/*    (M_IE_INDICATOR  ,M_IE_ISUP_IND):         */
           :B_IE_DISCR_IN       RANGE (0:255),
            B_IE_UPD_IND        M_IND_BYTE   ,
            B_IE_UPD_VAL        M_IND_BYTE    END
         ESAC);

