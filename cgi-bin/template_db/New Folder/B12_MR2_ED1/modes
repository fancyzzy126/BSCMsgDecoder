 B_FMM_ID M_FMM_ID,
    B_DEV_NBR    M_DEV_NBR , /* LOCAL TRX ID */
 /* NUMBER OF NEIGH CELLS ADDED IN THIS MESSAGE */
 B_MSG_NCELL M_BYTE,
           /* TOTAL NEIGHBOUR CELLS COUNTER ON THIS TRX */
 B_MAX_NCELL_CNT  M_BYTE,
  /* TOTAL SAMPLES FROM ALL NEIGHBOUR CELLS */
