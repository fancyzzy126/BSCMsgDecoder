NEWMODE M_CM_SERVICE = STRUCT
 ( B_SDU_ID                M_CM_SDU_ID,     /* SERVICES OF ENTITY X   */
   B_CTR_INFO              M_OSI_CTR_INFO,  /*          CONTROL FLAGS */
   B_CON_REF               M_OSI_REFERENCE, /*   CONNECTION REFERENCE */
   B_BITMAP                M_OSI_BITMAP,    /* OPT PARAMETER PRESENCE */
   B_UB_ENTRY              M_OSI_UD_REF,    /* REFER TO THE USER DATA */
   B_OTHER_ID              M_PROCESS_ID,    /* ID OF  SENDING PROCESS */
   CASE B_SDU_ID OF
   ( E_CM_GO,
     E_CM_NOGO,
     E_CM_OPEN,
     E_CM_CLOSE,
     E_CM_ACK,
     E_CM_TEST   ) :       B_CM_FLOW            M_CM_FLOW            END
   ( E_CM_INITIALIZE_REQ,
     E_CM_INITIALIZE_IND): B_CM_INITIALIZE_INIT M_CM_INITIALIZE_INIT END
   ( E_CM_INITIALIZE_RSP,
     E_CM_INITIALIZE_CNF): B_CM_INITIALIZE_RESP M_CM_INITIALIZE_RESP END
   ( E_CM_EVENT_REPORT_REQ,
     E_CM_EVENT_REPORT_IND,
     E_CM_EVENT_REPORT_RSP,
     E_CM_EVENT_REPORT_CNF,
     E_CM_SET_REQ,
     E_CM_SET_IND,
     E_CM_SET_RSP,
     E_CM_SET_CNF,
     E_CM_GET_REQ,
     E_CM_GET_IND,
     E_CM_GET_RSP,
     E_CM_GET_CNF,
     E_CM_ACTION_REQ,
     E_CM_ACTION_IND,
     E_CM_ACTION_RSP,
     E_CM_ACTION_CNF,
     E_CM_CREATE_REQ,
     E_CM_CREATE_IND,
     E_CM_CREATE_RSP,
     E_CM_CREATE_CNF,
     E_CM_DELETE_REQ,
     E_CM_DELETE_IND,
     E_CM_DELETE_RSP,
     E_CM_DELETE_CNF  )  : B_CM_DATA_TRANS      M_CM_DATA_TRANS      END
   ( E_CM_TERMINATE_REQ,
     E_CM_TERMINATE_IND) : B_CM_TERMINATE_REQ   M_CM_TERMINATE_REQ   END
    (E_CM_TERMINATE_RSP,
     E_CM_TERMINATE_CNF) : B_CM_TERMINATE_RSP   M_CM_TERMINATE_RSP   END
   ( E_CM_ABORT_REQ,
     E_CM_ABORT_IND,
     E_CM_FORCE_RELEASE) : B_CM_ABORT           M_CM_ABORT           END
     ESAC
 );

