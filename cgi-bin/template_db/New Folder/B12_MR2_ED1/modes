SYN  M_DPTC_RENUMBER_LIN_CH  M_DPTC_RENUM_ARRAY  =
        (: (0) : 21 , /* CH 0 : NOT USED FOR JOINS  */
           (1) : 22 ,
           (2) : 23 ,
           (3) : 24 ,
           (4) : 25 ,
           (5) : 26 ,   /*    RENUMBERING OF DPTC-CHANNELS :     */
           (6) : 27 ,   /*                                       */
           (7) : 28 ,   /* DPTC-CH ->----->-----> LIN OBCI-PORT  */
           (8) : 29 ,   /*                                       */
           (9) : 30 ,   /*     DPTC-CH + 21 = LIN CHANNEL        */
          (10) : 31 ,   /*             ----                      */
          (11) :  0 ,
          (12) :  1 ,
          (13) :  2 ,
          (14) :  3 ,
          (15) :  4 ,
          (16) :  5 , /* CH 16 : NOT USED FOR JOINS  */
          (17) :  6 ,
          (18) :  7 ,
          (19) :  8 ,
          (20) :  9 ,
          (21) : 10 ,
          (22) : 11 ,
          (23) : 12 ,
          (24) : 13 ,
          (25) : 14 ,
          (26) : 15 ,
          (27) : 16 ,
          (28) : 17 ,
          (29) : 18 ,
          (30) : 19 ,
          (31) : 20  :);

SYN  M_DPTC_RENUMBER_LOUT_CH  M_DPTC_RENUM_ARRAY =
        (: (0) : 31 , /* CH 0 : NOT USED FOR JOINS  */
           (1) :  0 ,
           (2) :  1 ,
           (3) :  2 ,
           (4) :  3 ,
           (5) :  4 ,   /*    RENUMBERING OF DPTC-CHANNELS :     */
           (6) :  5 ,   /*                                       */
           (7) :  6 ,   /* DPTC-CH <-----<-----<- LOUT OBCI-PORT */
           (8) :  7 ,   /*                                       */
           (9) :  8 ,   /*     DPTC-CH - 1 = LIN CHANNEL         */
          (10) :  9 ,   /*             ---                       */
          (11) : 10 ,
          (12) : 11 ,
          (13) : 12 ,
          (14) : 13 ,
          (15) : 14 ,
          (16) : 15 , /* CH 16 : NOT USED FOR JOINS  */
          (17) : 16 ,
          (18) : 17 ,
          (19) : 18 ,
          (20) : 19 ,
          (21) : 20 ,
          (22) : 21 ,
          (23) : 22 ,
