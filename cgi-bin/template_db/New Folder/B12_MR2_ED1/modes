                      B_RRFCN_FL             M_RRFCN_FL  PACK END
                     :B_NOT_USED_FSL         M_4BYTE_ARRAY,
                      B_RRFCN_FSL            M_RRFCN_FSL PACK END
                     :B_NOT_USED_FL2         M_2BYTE ,
                      B_NU_EXT_IND           RANGE(0:3)  PACK,
                      B_EXT_IND              BOOL        PACK,
                      B_EXT_SPARE            BOOL        PACK END
                     :B_NOT_USED_FL3         M_2BYTE ,
                      B_NU_EXT_IND_NGH       RANGE(0:3)  PACK,
                      B_EXT_IND_NGH          BOOL        PACK,
                      B_BA_IND_NGH           BOOL        PACK END
                      ESAC);

NEWMODE  M_FREQ_LIST_IE = STRUCT (
                   CASE OF
                     :B_FORMAT_ID_HIGH_FL    RANGE(0:3)  PACK,
                      B_SPARE_FL             RANGE(0:3)  PACK,
                      B_FORMAT_ID_LOW_FL     RANGE(0:7)  PACK,
                      B_ORIG_ARFCN_FL_HI     BIN(1)      PACK,
                      B_ORIG_ARFCN_FL_ME     BIN(8)      PACK,
                      B_ORIG_ARFCN_FL_LO     BIN(1)      PACK END
/*                    B_ORIG_ARFCN_FL        RANGE(0:1023) PACK END  */
                     :B_NOT_USED_FL          M_2BYTE ,
                      B_RRFCN_FL             M_RRFCN_FL  PACK END
                   ESAC );

SYN M_FREQ_LIST_IEID        M_GSM408_IEID = (B'00000101);

