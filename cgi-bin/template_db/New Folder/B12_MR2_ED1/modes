NEWMODE M_BSC_TERM_ERRCD = SET(
E_NO_ERROR_BSC_TERM,  /* 0, NO ERROR */
E_ABIS_OR_BTS_ON_ABTP,
/* 1, THERE IS ABIS OR BTS ON THE ABIS-HWAY-TP TO BE REMOVED */
E_OML_OR_RSL_ON_CCP,
/* 2, THERE IS OML OR RSL ON THE CCP TO BE REMOVED */
E_HSL_ON_ATERMUX, /* 3, THERE IS HSL ON THE ATERMUX TO BE REMOVED */
E_TC_ON_ATERMUX,
  /* 4, THERE IS TC CONNECTED ON THE CS ATERMUX TO BE REMOVED */
E_CANT_REMOVE_PS_ATERMUX,
  /* 5, THE ATERMUX TO BE REMOVED IS PS ATERMUX */
E_LACK_OF_CCP, /* 6, THERE IS NOT ENOUGH CCP TO BE REMOVED AS REQUEST */
E_FAIL_PERFORM_DEFENCE,
/* 7, FAILED TO PERFORM ACTION DEFENCE DURING BSC REDUCTION */
E_FAIL_PERFORM_RESET,
  /* 8, FAILED TO PERFORM ACTION RESET DURING BSC REDUCTION */
E_FAIL_PERFORM_LOCK,
 /* 9, FAILED TO PERFORM ACTION LOCK DURING BSC REDUCTION */
E_UNSEIZE_ATTP_FAIL,  /* 10, FAILED TO UNSEIZE ATER-HWAY-TP */
E_CCP_STATE_INCORRECT, /* 11, CCP STATE IS NOT IT/FIT */
E_ABIS_ATER_FLEX_ACT
  /* 12, IN ABIS_ATER_FLEX STATE,REFUSE TO REDUCTION TO 5 OR LESS */
);

