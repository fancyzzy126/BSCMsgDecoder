                          B_ERROR_TYPE M_CM_ERROR_TYPE ,
                          CASE B_ERROR_TYPE OF
                          (E_INV_REJECT_TYPE):
                             B_INV_RJCT_VAL M_RO_U_INV_PROBLEM END
                          (E_GEN_REJECT_TYPE):
                             B_GEN_RJCT_VAL M_RO_GEN_PROBLEM END
                          (E_LOC_REJECT_TYPE):
                             B_LOC_RJCT_VAL M_SMAE_LOC_PROBLEM END
                          (E_ERROR_TYPE):
                             B_ERROR_VAL M_ERROR_VAL_SET END
                                     ESAC);

